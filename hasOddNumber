function hasOddNumber(arr) {
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] % 2 !== 0) {
            return true;  // Return true immediately if an odd number is found
        }
    }
    return false;  // Return false if no odd numbers were found
}

// Test cases
console.log(hasOddNumber([1, 2, 2, 2, 2, 2, 4])); // true
console.log(hasOddNumber([2, 2, 2, 2, 2, 4]));    // false
