function hasOnlyOddNumbers(arr) {
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] % 2 === 0) {
            return false;  // Return false immediately if an even number is found
        }
    }
    return true;  // Return true if no even numbers were found
}

// Test cases
console.log(hasOnlyOddNumbers([1, 3, 5, 7])); // true
console.log(hasOnlyOddNumbers([1, 2, 3, 5, 7])); // false
